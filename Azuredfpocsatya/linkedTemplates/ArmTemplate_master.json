{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Azuredfpocsatya"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"AzureHDInsightLinkedService_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureHDInsightLinkedService'"
		},
		"AzureStaticHDInsightCluster_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureStaticHDInsightCluster'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfpocsatya.dfs.core.windows.net"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfpocsatya.dfs.core.windows.net"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "68a938d0-fbed-4563-985a-2097d93c7089"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "poc-resourcegroup"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "4bafa8c2-507b-4bd9-abca-cf35d00eb033"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "3cbcc3d3-094d-4006-9849-0d11d61f484d"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterSshUserName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"AzureStaticHDInsightCluster_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"AzureHDInsightLinkedService_clusterPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterPassword')]"
					},
					"AzureHDInsightLinkedService_clusterSshPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterSshPassword')]"
					},
					"AzureHDInsightLinkedService_servicePrincipalKey": {
						"value": "[parameters('AzureHDInsightLinkedService_servicePrincipalKey')]"
					},
					"AzureStaticHDInsightCluster_password": {
						"value": "[parameters('AzureStaticHDInsightCluster_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_tenant')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterUserName')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterSshUserName": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterSshUserName')]"
					},
					"AzureStaticHDInsightCluster_properties_typeProperties_userName": {
						"value": "[parameters('AzureStaticHDInsightCluster_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}